{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "commitBody": "Generated by renovateBot",
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": false
    },
    {
      "packagePatterns": [
        "^golang.org/x/"
      ],
      "schedule": [
        "on the first day of the month"
      ]
    }
  ],
  "ignorePaths": [
  ]
}
